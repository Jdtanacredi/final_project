<table class="table">
  <thead>
    <tr>
      <th>User Email</th>
      <th>Is Admin?</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
      <td><%= button_to 'Destroy', admin_user_path(user), method: :delete %></td>
    </tr>
  </tbody>
  <% end %>
</table>